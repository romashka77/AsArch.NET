@model AsArch.NET.Models.NodeEditViewModels

<div class="form-group">
    @Html.HiddenFor(model => Model.Attrs[Model.IdAttr].IdAttr)
    @Html.HiddenFor(model => Model.Attrs[Model.IdAttr].IdAttrType)
    @Html.HiddenFor(model => Model.Attrs[Model.IdAttr].NameAttr)
    @Html.LabelFor(model => Model.Attrs[Model.IdAttr].NameAttr, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownListFor(model => model.Attrs[Model.IdAttr].RefNode, ViewBag.ListCodIska as IEnumerable<SelectListItem>, "Выберите код иска", htmlAttributes: new { @class = "form-control" })
    </div>
</div>

